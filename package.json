{
  "name": "kuzzleio-documentation",
  "version": "3.1.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "vuepress dev src --no-cache --no-clear-screen",
    "doc-dev": "npm run dev",
    "build": "vuepress build src --no-cache",
    "doc-build": "npm run build",
    "debug": "node --nolazy --inspect-brk=9229 ./node_modules/.bin/vuepress dev src",
    "dead-links": "ruby .ci/dead-links.rb -p src/",
    "doc-upload": "aws s3 sync src/.vuepress/dist s3://$S3_BUCKET",
    "doc-cloudfront": "aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_DISTRIBUTION_ID --paths '/*'",
    "doc-deploy": "npm run upload && npm run cloudfront"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kuzzleio/documentation.git"
  },
  "author": "The Kuzzle Team <support@kuzzle.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "algoliasearch": "^4.19.1",
    "cheerio": "^1.0.0-rc.12",
    "clipboard": "^2.0.11",
    "commander": "^11.0.0",
    "docsearch.js": "^2.6.3",
    "fix-indents": "^0.1.2092",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "node-sass": "8.0.0",
    "sass-loader": "10.4.1",
    "vuepress": "1.5.2",
    "vuepress-frontmatter-lint": "^2.1.2"
  }
}
